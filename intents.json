{"intents":
 [

        {"tag": "greeting",

         "patterns": ["Hi there", "How are you", "Is anyone there?","Hey","Hola", "Hello", "Good day"],

         "responses": ["Hello, thanks for asking", "Good to see you again", "Hi there, how can I help?"],

         "context": [""]

        },

        {"tag": "goodbye",

         "patterns": ["Bye", "See you later", "Goodbye", "Nice chatting to you, bye", "Till next time"],

         "responses": ["See you!", "Have a nice day", "Bye! Come back again soon.", "Nice chatting with you"],

         "context": [""]

        },

        {"tag": "thanks",

         "patterns": ["Thanks", "Thank you", "That's helpful", "Awesome, thanks", "Thanks for helping me"],

         "responses": ["Happy to help!", "Any time!", "My pleasure"],

         "context": [""]

        },

        {"tag": "noanswer",

         "patterns": [".", "'", "[", "]", "{", "}", "+"],

         "responses": ["Sorry, can't understand you", "Please tell me more", "Not sure I understand you", "Can you rephrase it?"],

         "context": [""]

        },

        {"tag": "help",

         "patterns": ["How you could help me?", "What you can do?", "What help you provide?", "Can you help me?", "How you can be helpful?", "What kind of helps you offer"],

         "responses": ["I am not good at helping", "You need help! I don't think I'm the right person for it", "Haha! Don't joke with me. I can't help you", "May be you should consider using /help instead of asking help"],

         "context": [""]

        },
	{"tag": "right",

         "patterns": ["ok", "k", "right", "that's it", "exactly", "now you see"],

         "responses": ["OK !", "Cool", "Haha!", "Right?", "That's it?", "Nice to see"],

         "context": [""]

        },
	{"tag": "name",

         "patterns": ["who are you?", "what is you?", "what is your name", "Can I know your name?"],

         "responses": ["Don't you know me?", "Haha! Good question", "I'm a telegram bot", "I like your question. I'm a bot", "Nice you asked. But I can't reply you"],

         "context": [""]

        },
	{"tag": "from",

         "patterns": ["where are you from?", "where do you live?", "you from which place", "can I know from where you are?"],

         "responses": ["I would like to ask you the same", "Don't question my origin", "I'm from where you can't reach", "Unfortunately, I don't have answer for it"],

         "context": [""]

        }
 ]

}
